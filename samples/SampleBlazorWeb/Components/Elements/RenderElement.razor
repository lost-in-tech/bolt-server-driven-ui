@using Bolt.ServerDrivenUI.Core.Elements


@switch (this.Element)
{
    case Sample.Elements.Stack:
        <Stack Data=@Element Sections=@Sections/>
        break;
    case Sample.Elements.Paragraph:
        <Paragraph Data=@Element />
        break;
    case Bolt.ServerDrivenUI.Core.Elements.Placeholder:
        <Placeholder Data=@Element Sections=@Sections />
        break;
}


@code {
    [Parameter]
    public required IElement Element { get; set; }
    [Parameter]
    public required Dictionary<string, IElement> Sections { get; set; }
}